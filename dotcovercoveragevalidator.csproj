<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>dotcovercoveragecheck</ToolCommandName>
    <PackageOutputPath>./nupkg</PackageOutputPath>
    <PackageId>Kidde82.DotCoverCoverageValidator</PackageId>
    <Authors>Christian Persson</Authors>
    <Company>Christian Persson</Company>
    <PackageDescription>Package for check test coverage threshold!</PackageDescription>
    <RepositoryUrl>https://github.com/Kidde82/DotCoverCoverageValidator</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
